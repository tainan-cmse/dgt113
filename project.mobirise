          "_customHTML": "<section class=\"tabs2\" group=\"Schedule\" data-bg-video=\"{{bg.type == 'video' && bg.value.url}}\" mbr-class=\"{'mbr-parallax-background': bg.parallax}\" plugins=\"mbr-tabs\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <header>Title</header>\n        <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Show Buttons\" name=\"showButtons\">\n        <header>Tabs</header>\n        <input type=\"color\" title=\"Tabs Color\" name=\"tabsColor\" value=\"#f6f6f6\">\n        <input type=\"color\" title=\"Active Tab Color\" name=\"activeTabColor\" value=\"#7af790\">\n        <select title=\"Tabs Count\" name=\"bootstrapTabs\">\n            <option value=\"1\">1</option>\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected>5</option>\n        </select>\n        <input type=\"range\" inline title=\"Schedule Count Tab 1\" name=\"scheduleCount1\" min=\"1\" max=\"30\" step=\"1\" value=\"10\">\n        <input type=\"range\" inline title=\"Schedule Count Tab 2\" name=\"scheduleCount2\" min=\"1\" max=\"30\" step=\"1\" value=\"3\" condition=\"bootstrapTabs > 1\">\n        <input type=\"range\" inline title=\"Schedule Count Tab 3\" name=\"scheduleCount3\" min=\"1\" max=\"30\" step=\"1\" value=\"5\" condition=\"bootstrapTabs > 2\">\n        <input type=\"range\" inline title=\"Schedule Count Tab 4\" name=\"scheduleCount4\" min=\"1\" max=\"30\" step=\"1\" value=\"4\" condition=\"bootstrapTabs > 3\">\n        <input type=\"range\" inline title=\"Schedule Count Tab 5\" name=\"scheduleCount5\" min=\"1\" max=\"30\" step=\"1\" value=\"2\" condition=\"bootstrapTabs > 4\">\n        <input type=\"color\" title=\"Border Color\" name=\"borderColor\" value=\"#f6f6f6\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Background Image\" value=\"@PROJECT_PATH@/assets/images/conference28-1920.jpg\">\n            <input type=\"video\" title=\"Background Video\" value=\"https://www.youtube.com/watch?v=36YgDDJ7XSc\">\n            <input type=\"color\" title=\"Background Color\" value=\"#232323\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#149dcc\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n\n    <div class=\"container\">\n        <div class=\"row title__wrap\" mbr-if=\"showSubtitle || showTitle || showButtons\">\n            <div class=\"title__block\" mbr-class=\"{'col-sm-6' : showButtons, 'col-sm-12' : !showButtons}\">\n                <h3 class=\"mbr-section-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-4\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">EXCURSION</h3>\n                <h2 class=\"mbr-section-title mbr-bold mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">活動</h2>\n            </div>\n     